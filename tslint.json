{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "defaultSeverity": "warning",
  "rules": {
    "interface-name": [
      false
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
